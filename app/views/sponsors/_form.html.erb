<%= simple_form_for(@sponsor) do |f| %>
  <%= f.error_notification %>

<p>Note: When entering the website URL, do not include http://</p>
  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :website %> 
    <%= select( :sponsor, :level, [['Gold', 1], ['Silver', 2]]) %>
  </div></br>
  <div class="form-inputs">
    <%= f.file_field :logo %>
  </div>
</br>
  <div class="form-actions">
    <%= f.button :submit %>
  </div>

<% end %>
</br>
