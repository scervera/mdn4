                <div class="sponsor-caption">
                    <h5><%= sponsor.name %></h5>
                </div>
                <div class="sponsor-thumb">
                  <% unless sponsor.logo_url(:portrait).to_s.length == 0 %>
                      <figure><%= link_to( image_tag(sponsor.logo_url(:portrait).to_s), "http://#{sponsor.website}", :target => "_blank") %> </figure>
                  <% else %>
                      <figure><%= link_to( image_tag(sponsor.logo_url(:landscape).to_s), "http://#{sponsor.website}", :target => "_blank") %> </figure>
                  <% end %>
                  
                </div>
              </div>
            <% end %>
            <!-- This ends the loop for each sponsor in the collection -->
          </div>
        </div>
      </div>
  <div class="col-xs-1"></div>
</div>
